  <Text UId="666">- CTW - &gt;  to -&gt; Global CMD ///////From auto array CTW_BIT</Text>
  <NewLine UId="667" />
  <Blank Num="4" UId="668" />
  <LineComment Inserted="true" UId="669">
    <Text UId="670">Motor MODE - CMD From auto array db</Text>
  </LineComment>
  <NewLine UId="671" />
  <Blank Num="4" UId="672" />
  <LineComment UId="673">
    <Text UId="674"> IF #ts_AutoControl.Global_CMD = 0 AND</Text>
  </LineComment>
  <NewLine UId="675" />
  <Blank Num="4" UId="676" />
  <LineComment UId="677">
    <Text UId="678">     BYTE_TO_INT(#ts_AutoControl.CTW.%B2 AND 2#01111111) &lt;&gt; 0 THEN</Text>
  </LineComment>
  <NewLine UId="679" />
  <Blank Num="4" UId="680" />
  <LineComment UId="681">
    <Text UId="682">     CASE BYTE_TO_INT(#ts_AutoControl.CTW.%B2 AND 2#01111111) OF</Text>
  </LineComment>
  <NewLine UId="683" />
  <Blank Num="4" UId="684" />
  <LineComment UId="685">
    <Text UId="686">         1:  (*bit 0 Manual =  Function change to Manual mode - активує ManualRemote*)</Text>
  </LineComment>
  <NewLine UId="687" />
  <Blank Num="4" UId="688" />
  <LineComment UId="689">
    <Text UId="690">             #ts_AutoControl.Global_CMD := 1;</Text>
  </LineComment>
  <NewLine UId="691" />
  <Blank Num="4" UId="692" />
  <LineComment UId="693">
    <Text UId="694">         2:  (*bit 1 AUTO = Auto Function change to Auto mode*)</Text>
  </LineComment>
  <NewLine UId="695" />
  <Blank Num="4" UId="696" />
  <LineComment UId="697">
    <Text UId="698">             #ts_AutoControl.Global_CMD := 2;</Text>
  </LineComment>
  <NewLine UId="699" />
  <Blank Num="4" UId="700" />
  <LineComment UId="701">
    <Text UId="702">             //4:  (*bit 2 Перевести в ManualLocal - ручний зі щита кнопками*)</Text>
  </LineComment>
  <NewLine UId="703" />
  <Blank Num="4" UId="704" />
  <LineComment UId="705">
    <Text UId="706">             //    #ts_AutoControl.Global_CMD := 3;</Text>
  </LineComment>
  <NewLine UId="707" />
  <Blank Num="4" UId="708" />
  <LineComment UId="709">
    <Text UId="710">         8:  (*bit 3 Simulation - ON Симуляція*)</Text>
  </LineComment>
  <NewLine UId="711" />
  <Blank Num="4" UId="712" />
  <LineComment UId="713">
    <Text UId="714">             #ts_AutoControl.Global_CMD := 7;</Text>
  </LineComment>
  <NewLine UId="715" />
  <Blank Num="4" UId="716" />
  <LineComment UId="717">
    <Text UId="718">         16:  (*bit 4 Simulation - OFF Симуляція*)</Text>
  </LineComment>
  <NewLine UId="719" />
  <Blank Num="4" UId="720" />
  <LineComment UId="721">
    <Text UId="722">             #ts_AutoControl.Global_CMD := 8;</Text>
  </LineComment>
  <NewLine UId="723" />
  <Blank Num="4" UId="724" />
  <LineComment UId="725">
    <Text UId="726">             //32: (*bit 5 DSABLE/OFF/STOP*)</Text>
  </LineComment>
  <NewLine UId="727" />
  <Blank Num="4" UId="728" />
  <LineComment UId="729">
    <Text UId="730">             //    ;</Text>
  </LineComment>
  <NewLine UId="731" />
  <Blank Num="4" UId="732" />
  <LineComment UId="733">
    <Text UId="734">         64:  (*bit 6 DSABLE/OFF/STOP*)</Text>
  </LineComment>
  <NewLine UId="735" />
  <Blank Num="4" UId="736" />
  <LineComment UId="737">
