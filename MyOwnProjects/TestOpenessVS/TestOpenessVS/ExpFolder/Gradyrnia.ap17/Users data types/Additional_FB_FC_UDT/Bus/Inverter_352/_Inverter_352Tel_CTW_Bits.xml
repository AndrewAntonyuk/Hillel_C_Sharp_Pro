  <Section Name="None">
    <Member Name="OFF1" Datatype="Bool">
      <Comment>
        <MultiLanguageText Lang="en-US">Bit 0  - 0 = Switch-off  with ramp-down p1121</MultiLanguageText>
      </Comment>
      <StartValue>false</StartValue>
    </Member>
    <Member Name="OFF2" Datatype="Bool">
      <Comment>
        <MultiLanguageText Lang="en-US">Bit 1  - 0 = Switch-off immediately, motor coast to stop</MultiLanguageText>
      </Comment>
      <StartValue>true</StartValue>
    </Member>
    <Member Name="OFF3" Datatype="Bool">
      <Comment>
        <MultiLanguageText Lang="en-US">Bit 2  - 0 = Quick stop with ramp-down p1135</MultiLanguageText>
      </Comment>
      <StartValue>true</StartValue>
    </Member>
    <Member Name="EnableOperation" Datatype="Bool">
      <Comment>
        <MultiLanguageText Lang="en-US">Bit 3  - 1 = Switch-on motor (pulses can be enabled)</MultiLanguageText>
      </Comment>
      <StartValue>true</StartValue>
    </Member>
    <Member Name="EnableRFG" Datatype="Bool">
      <Comment>
        <MultiLanguageText Lang="en-US">Bit 4 - 1 = Enable ramp-function generator</MultiLanguageText>
      </Comment>
      <StartValue>true</StartValue>
    </Member>
    <Member Name="FrozeRFG" Datatype="Bool">
      <Comment>
        <MultiLanguageText Lang="en-US">Bit 5 - 0 = Ramp-function generator output is frozen</MultiLanguageText>
      </Comment>
      <StartValue>true</StartValue>
    </Member>
    <Member Name="EnableSetpoint" Datatype="Bool">
      <Comment>
        <MultiLanguageText Lang="en-US">Bit 6 - 1 = Motor accelerates with ramp p1120; 0 = Motor brakes with ramp p1121</MultiLanguageText>
      </Comment>
      <StartValue>true</StartValue>
    </Member>
    <Member Name="Acknowlege" Datatype="Bool">
      <Comment>
        <MultiLanguageText Lang="en-US">Bit 7 - 1 = Acknowlege faults with positive edge. If the ON command still active, the inverter switches to "closing lockout" state</MultiLanguageText>
      </Comment>
    </Member>
    <Member Name="Bit8_NotUsed" Datatype="Bool">
      <Comment>
        <MultiLanguageText Lang="en-US">Bit 8 - NU</MultiLanguageText>
      </Comment>
    </Member>
    <Member Name="Bit9_NotUsed" Datatype="Bool">
      <Comment>
        <MultiLanguageText Lang="en-US">Bit 9 - NU</MultiLanguageText>
      </Comment>
    </Member>
    <Member Name="CtrlByPLC" Datatype="Bool">
      <Comment>
        <MultiLanguageText Lang="en-US">Bit 10 - 1 = Control via fieldbus, process data valid</MultiLanguageText>
      </Comment>
      <StartValue>true</StartValue>
    </Member>
    <Member Name="ChangeDir" Datatype="Bool">
      <Comment>
        <MultiLanguageText Lang="en-US">Bit 11 - 1 = Change direction. Setpoint is inverted in the drive</MultiLanguageText>
      </Comment>
    </Member>
    <Member Name="Bit12_NotUsed" Datatype="Bool">
      <Comment>
        <MultiLanguageText Lang="en-US">Bit 12 - NU</MultiLanguageText>
      </Comment>
    </Member>
    <Member Name="MOP_Up" Datatype="Bool">
      <Comment>
        <MultiLanguageText Lang="en-US">Bit 13 - 1 = Motorized potentiometer is increased</MultiLanguageText>
      </Comment>
    </Member>
    <Member Name="MOP_Dwn" Datatype="Bool">
      <Comment>
        <MultiLanguageText Lang="en-US">Bit 14 - 1 = Motorized potentiometer is decreased</MultiLanguageText>
